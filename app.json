{
  "expo": {
    "name": "StarBoundConquest",
    "slug": "StarBoundConquest",
    "version": "1.1.0",
    "orientation": "portrait",
    "newArchEnabled": true,
    "icon": "./assets/images/SC_logo1SQ.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/SC_logo1.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SC_logo1SQ.png",
        "backgroundColor": "#000000"
      },
      "package": "com.happybob59.StarBoundConquest"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/SC_logo1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Aboreto-Regular.ttf",
            "./assets/fonts/LegacySpartan-Black.ttf",
            "./assets/fonts/LegacySpartan-Bold.ttf",
            "./assets/fonts/LegacySpartan-Light.ttf",
            "./assets/fonts/LegacySpartan-Medium.ttf",
            "./assets/fonts/LegacySpartan-Regular.ttf",
            "./assets/fonts/LegacySpartan-SemiBold.ttf",
            "./assets/fonts/LegacySpartan-ExtraBold.ttf",
            "./assets/fonts/LegacySpartan-Thin.ttf",
            "./assets/fonts/SpaceMono-Regular.ttf",
            "./assets/fonts/SpaceMono-Italic.ttf",
            "./assets/fonts/SpaceMono-Bold.ttf"
          ]
        }
      ],
      "@react-native-google-signin/google-signin",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "34d8afa6-b067-467c-95c1-f8627513bc00"
      }
    },
    "owner": "happybob59"
  },
  "owner": "happybob59"
}
